{
  "name": "yellowbird-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "pnpm db:seed:generate && npx supabase db reset && pnpm generate:types:local",
    "db:seed:generate": "rm -f ./supabase/seed.sql && pnpm db:seed:sync && npx tsx seed.ts > supabase/seed.sql",
    "db:seed:sync": "npx @snaplet/seed sync",
    "backend:start": "npx supabase start",
    "backend:stop": "npx supabase stop",
    "backend:rebuild": "npx supabase stop --no-backup && pnpm backend:start && pnpm db:reset",
    "generate:types": "npx supabase gen types typescript --linked > src/database.types.ts",
    "generate:types:local": "npx supabase gen types typescript --local > src/database.types.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@next/env": "^14.2.6",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.45.2",
    "@szhsin/react-menu": "^4.2.2",
    "@tanstack/react-table": "^8.20.1",
    "@uiw/react-md-editor": "^4.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.368.0",
    "next": "^14.2.6",
    "next-auth": "5.0.0-beta.16",
    "next-remove-imports": "^1.0.12",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "rehype-sanitize": "^6.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-radix": "^3.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@parcel/watcher": "^2.4.1",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "0.96.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "pg": "^8.12.0",
    "postcss": "^8.4.41",
    "supabase": "^1.190.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "child_process": false,
    "net": false,
    "tls": false
  }
}
