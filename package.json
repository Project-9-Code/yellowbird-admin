{
  "name": "yellowbird-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "pnpm db:seed:generate && npx supabase db reset",
    "db:seed:generate": "rm -f ./supabase/seed.sql && pnpm db:seed:sync && npx env-cmd -f ./.env.local npx tsx seed.ts > supabase/seed.sql",
    "db:seed:sync": "npx env-cmd -f ./.env.local npx @snaplet/seed sync",
    "backend:start": "npx supabase start",
    "backend:stop": "npx supabase stop",
    "backend:rebuild": "npx supabase stop --no-backup && pnpm backend:start && pnpm db:reset",
    "generate:types": "npx supabase gen types typescript --linked > src/database.types.ts",
    "generate:types:local": "npx supabase gen types typescript --local > src/database.types.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/themes": "^3.0.2",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.42.7",
    "@szhsin/react-menu": "^4.1.0",
    "@tanstack/react-table": "^8.16.0",
    "@uiw/react-md-editor": "^4.0.4",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.368.0",
    "next": "^14.2.3",
    "next-auth": "5.0.0-beta.16",
    "next-remove-imports": "^1.0.12",
    "postgres": "^3.4.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "rehype-sanitize": "^6.0.0",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-radix": "^3.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "0.96.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.20.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "supabase": "^1.164.1",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "child_process": false,
    "net": false,
    "tls": false
  }
}
